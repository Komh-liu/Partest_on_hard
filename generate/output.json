        "code": "#include <iostream>\n#include <vector>\n#include <queue>\n#include <omp.h>\n\nvoid bfs(const Graph& graph, int start, std::vector<int>& result) {\n    std::vector<bool> visited(graph.numVertices, false);\n    std::queue<int> q;\n\n    q.push(start);\n    visited[start] = true;\n\n    while (!q.empty()) {\n        int current = q.front();\n        q.pop();\n\n        #pragma omp critical\n        result.push_back(current);\n\n        int begin = graph.offset[current];\n        int end = graph.offset[current + 1];\n\n        #pragma omp parallel for\n        for (int i = begin; i < end; ++i) {\n            int neighbor = graph.edges[i];\n            if (!visited[neighbor]) {\n                #pragma omp critical\n                {\n                    if (!visited[neighbor]) { // Double check in critical section\n                        q.push(neighbor);\n                        visited[neighbor] = true;\n                    }\n                }\n            }\n        }\n    }\n\n    #pragma omp single\n    std::cout << result.size() << std::endl;\n}",
